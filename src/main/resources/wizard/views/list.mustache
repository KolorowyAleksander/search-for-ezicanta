<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <title></title>
    <meta name="author" content="">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link rel="stylesheet" href="/assets/main.css">
</head>

<body>
<div>
    <ol>
        {{#pages}}
            <li>
                <a href="{{url}}">{{title}}</a><span>{{language}}</span>
                <div>{{contentsShort}}</div>

                <span>TF-IDF:</span><span class="kappa">{{tfidfScore}}</span>
                <span>lucene:</span><span class="kappa">{{luceneScore}}</span>
                <span>PageRank:</span><span class="kappa">{{pageRankScore}}</span>
                <span>incoming:</span><span class="kappa">{{incoming}}</span>
                <span>outgoing:</span><span class="kappa">{{outgoing}}</span>
            </li>
        {{/pages}}
    </ol>
</div>

<!-- Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</body>

</html>